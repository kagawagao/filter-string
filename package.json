{
  "name": "filter-string",
  "version": "1.0.0",
  "description": "Parse and stringify RESTFul filter string",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test && nyc ava --verbose",
    "flow": "flow",
    "lint": "eslint ./src",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublishOnly": "npm run flow && npm run lint && npm run test",
    "build": "cross-env NODE_ENV=production && npm run compile && npm run compile:es && rollup -c",
    "compile": "babel src -d lib --presets=env,stage-0,flow --plugins=add-module-exports",
    "compile:es": "babel src -d es",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kagawagao/filter-string.git"
  },
  "keywords": [
    "parser",
    "stringify",
    "restful",
    "query",
    "filter"
  ],
  "author": "kagawagao <kingsongao1221@gmail.com> (http://kingsongao.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kagawagao/filter-string/issues"
  },
  "homepage": "https://github.com/kagawagao/filter-string#readme",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "flow-bin": "^0.71.0",
    "nyc": "^11.7.1",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "engines": {
    "node": ">= 4",
    "npm": ">= 5"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "browserslist": "ie >= 10, last 2 Chrome versions, last 2 Firefox versions",
  "peerDependencies": {
    "core-js": "^2.5.1"
  }
}
